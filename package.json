{
  "name": "react-slider-puzzle",
  "version": "1.0.1",
  "private": true,
  "author": "Barry Gilbert",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "test:coverage": "react-scripts test --watchAll=false --coveragePathIgnorePatterns serviceWorker.js --coverage"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "semi": [
        1,
        "always"
      ],
      "max-len": [
        1,
        80,
        2
      ],
      "object-curly-spacing": [
        1,
        "always",
        {}
      ],
      "space-in-parens": [
        1,
        "never"
      ],
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "react/prop-types": [
        1
      ],
      "prefer-template": [
        1
      ],
      "prefer-destructuring": [
        1
      ],
      "prefer-const": [
        1
      ],
      "no-var": [
        1
      ],
      "import/order": [
        "error",
        {
          "groups": [
            [
              "external",
              "builtin"
            ],
            "internal",
            [
              "sibling",
              "parent"
            ],
            "index"
          ]
        }
      ]
    }
  },
  "eslintIgnore": [
   "serviceWorker.js",
   "localeData.js",
   "oldLocaleData.js"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/node": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@google-cloud/translate": "^4.1.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest-localstorage-mock": "^2.4.0"
  }
}
